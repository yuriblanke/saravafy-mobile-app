{
  "name": "saravafy",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env APP_VARIANT=production expo start",
    "start:dev": "cross-env APP_VARIANT=dev expo start --dev-client",
    "android": "cross-env APP_VARIANT=production expo run:android",
    "android:dev": "cross-env APP_VARIANT=dev expo run:android",
    "android:prod": "cross-env APP_VARIANT=production expo run:android --variant=release",
    "ios": "cross-env APP_VARIANT=production expo run:ios",
    "ios:dev": "cross-env APP_VARIANT=dev expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "eas:dev": "eas build -p android --profile development",
    "eas:preview": "eas build -p android --profile preview",
    "eas:prod": "eas build -p android --profile production",
    "eas:build:list": "eas build:list",
    "eas:build:download": "eas build:download",
    "eas-build-pre-build": "node ./scripts/eas-lint.mjs"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/material-top-tabs": "^7.4.11",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.15",
    "expo": "~54.0.30",
    "expo-auth-session": "^7.0.10",
    "expo-av": "~16.0.8",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-navigation-bar": "~5.0.10",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-native": "0.81.5",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-tab-view": "^4.2.2",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^10.0.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "engines": {
    "node": ">=20 <21"
  }
}
